@model IEnumerable<Dziekanat.Models.Users>

<div class="table-responsive">
    <table class="table table-striped table-bordered">
        <thead>
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var instructor in Model)
            {
                <tr>
                    <td>@instructor.Username</td>
                    <td>@instructor.Password</td>
                </tr>
            }
        </tbody>
    </table>x
</div>